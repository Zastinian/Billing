{
    "name": "zastinian/hedystiabilling",
    "description": "Billing software specially made for Pterodactyl panel",
    "homepage": "https://hedystia.com",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Zastinian",
            "email": "contact@zastinian.com",
            "homepage": "https://zastinian.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "contact@hedystia.com",
        "source": "https://github.com/Zastinian/HedystiaBilling",
        "docs": "https://docs.hedystia.com"
    },
    "require": {
        "php": "^8.2|^8.3",
        "cerbero/command-validator": "^2.5",
        "laravel/framework": "^10.48",
        "laravel/tinker": "^2.9",
        "mercadopago/dx-php": "3.0.7",
        "predis/predis": "^2.2",
        "queueworker/sansdaemon": "^1.2",
        "srmklive/paypal": "~1.0"
    },
    "require-dev": {
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Extensions\\": "extensions/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
