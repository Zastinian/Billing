{
  "name": "billing",
  "type": "module",
  "version": "develop",
  "scripts": {
    "dev": "astro dev",
    "start": "bun run server.ts",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check --minimumSeverity --minimumFailingSeverity && tsc --noEmit && biome ci ./src --organize-imports-enabled=false",
    "lint": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "prepare:husky": "husky",
    "db:create": "typeorm migration:create src/database/migrations/<name>",
    "db:entity": "typeorm entity:create src/database/entities/<name>",
    "db:migrate": "typeorm migration:run --dataSource src/database/data-source.ts",
    "key:generate": "bun run ./src/scripts/key.ts"
  },
  "dependencies": {
    "@astrojs/node": "^8.3.3",
    "astro": "^4.15.4",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "5.2.0",
    "mssql": "^11.0.1",
    "mysql2": "^3.11.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.2",
    "@biomejs/biome": "^1.8.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.4.0",
    "husky": "^9.1.4",
    "typescript": "^5.5.4"
  }
}
